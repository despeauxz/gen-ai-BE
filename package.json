{
  "name": "gen-ai-be",
  "version": "1.0.0",
  "main": "index.js",
  "author": "vbv000406 <despeauxz@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.19.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "test": "jest",
    "start": "npm run migration:up && npm run seed:up && babel-node --experimental-modules index.js",
    "dev": "nodemon --exec babel-node index.js",
    "lint": "eslint --fix app/ --ext .js",
    "precommit": "npm run lint && npm t",
    "prepare": "npm run migration:up && husky install",
    "migration:create": "db-migrate create --sql-file=true -m ./app/migrations/",
    "migration:up": "db-migrate up -m ./app/migrations/",
    "migration:down": "db-migrate down -c 100 -m ./app/migrations/",
    "migration:reset": "db-migrate reset -m ./app/migrations/",
    "migration:refresh": "npm run migration:reset  -m ./app/migrations/ && npm run migration:up -m ./app/migrations/",
    "seed:create": "db-migrate create:seed --config database.json -m ./app/migrations/",
    "seed:up": "db-migrate up:seed --config database.json -m ./app/migrations/",
    "seed:down": "db-migrate down:seed -c 100 --config database.json -m ./app/migrations/",
    "seed:reset": "db-migrate reset:seed --config database.json -m ./app/migrations/",
    "seed:refresh": "npm run seed:reset  -m ./app/migrations/ && npm run seed:up -m ./app/migrations/"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "debug": "^4.4.3",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^8.1.0",
    "file-stream-rotator": "^1.0.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "pg-monitor": "^3.1.0",
    "pg-promise": "^12.2.0",
    "response-time": "^2.3.4",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@types/jest": "^29.5.14",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "del": "^7.0.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "sinon": "^15.0.1",
    "supertest": "^6.0.1"
  }
}
